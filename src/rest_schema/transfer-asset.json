    "channelID",
    "assetID"
  ],
  "additionalProperties": false,
  "properties": {
    "transferDescription": {
      "type": "string"
    },
    "repoID": {
      "type": "string",
      "minLength": 1
    },
    "channelID": {
      "type": "string",
      "minLength": 1
    },
    "assetID": {
      "type": "string",
      "minLength": 1
    }
  }
}